<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="?attr/actionBarSize">



    <ImageView
        android:id="@+id/imageView"
        android:layout_width="147dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/yummy" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:ignore="MissingConstraints" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="34dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView">

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="365dp"
            android:layout_height="400dp">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="172dp"
                android:layout_height="81dp"
                app:layout_column="0"
                app:layout_row="2"
                app:srcCompat="@drawable/calamar" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="172dp"
                android:layout_height="81dp"
                app:layout_column="0"
                app:layout_row="4"
                app:srcCompat="@drawable/assiete" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="172dp"
                android:layout_height="81dp"
                app:layout_column="0"
                app:layout_row="6"
                app:srcCompat="@drawable/italien" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="172dp"
                android:layout_height="81dp"
                app:layout_column="1"
                app:layout_row="4"
                app:srcCompat="@drawable/raclette" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="172dp"
                android:layout_height="81dp"
                app:layout_column="1"
                app:layout_row="2"
                app:srcCompat="@drawable/tajine" />

            <ImageButton
                android:id="@+id/imageButton1"
                android:layout_width="172dp"
                android:layout_height="81dp"
                app:layout_column="0"
                app:layout_row="0"
                app:srcCompat="@drawable/maki" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="173dp"
                android:layout_height="81dp"
                app:layout_column="1"
                app:layout_row="0"
                app:srcCompat="@drawable/grec" />

            <ImageButton
                android:id="@+id/imageButton9"
                android:layout_width="173dp"
                android:layout_height="81dp"
                app:layout_column="0"
                app:layout_row="8"
                app:srcCompat="@drawable/spaghetti" />

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="173dp"
                android:layout_height="81dp"
                app:layout_column="0"
                app:layout_row="10"
                app:srcCompat="@drawable/tapas" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Restaurant"
                android:textAlignment="center"
                app:layout_column="1"
                app:layout_row="1" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="173dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Restaurant"
                android:textAlignment="center"
                app:layout_column="0"
                app:layout_row="3" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="173dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Restaurant"
                android:textAlignment="center"
                app:layout_column="0"
                app:layout_row="5" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="173dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Restaurant"
                android:textAlignment="center"
                app:layout_column="0"
                app:layout_row="1" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="173dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Restaurant"
                android:textAlignment="center"
                app:layout_column="0"
                app:layout_row="7" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="173dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Restaurant"
                android:textAlignment="center"
                app:layout_column="0"
                app:layout_row="9" />

        </androidx.gridlayout.widget.GridLayout>

    </ScrollView>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="411dp"
        android:layout_height="50dp"
        android:background="#00897B"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

            <ImageButton
                android:id="@+id/imageButton_1"
                android:layout_width="59dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/yummy" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>